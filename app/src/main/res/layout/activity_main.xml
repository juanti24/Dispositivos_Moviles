<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/background_dm"
    tools:context=".ui.activities.MainActivity">

    <TextView
        android:id="@+id/textView2"
        style="@style/textTitle"
        android:layout_width="362dp"
        android:layout_height="120dp"
        android:layout_marginTop="28dp"
        android:fontFamily="@font/roboto"
        android:text="Fast and Delicious Food"
        android:textSize="48sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.489"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        style="@style/Search"
        android:id="@+id/txt_buscar"
        android:layout_width="188dp"
        android:layout_height="49dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="24dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/imageButton3"
        style="@style/ButtonSet"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        app:layout_constraintStart_toEndOf="@+id/txt_buscar"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:srcCompat="@drawable/round_tune"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button

        android:id="@+id/button"
        style="@style/ButtonCancel"
        android:layout_width="90dp"
        android:layout_height="50dp"
        android:layout_marginStart="84dp"
        android:layout_marginTop="24dp"
        android:text="Button"
        app:layout_constraintStart_toEndOf="@+id/txt_buscar"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        tools:ignore="MissingConstraints" />

